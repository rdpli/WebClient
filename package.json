  "scripts": {
    "postinstall": "./tasks/checkDependencies.js",
    "lint": "eslint src/app src/helpers --ext .js --quiet --cache",
    "e2e": "./tasks/lazyInstallVendor e2e && cypress run",
    "pretest": "./tasks/lazyInstallVendor test && del ./build && cross-env NODE_ENV=test npm run config",
    "test": "cross-env NODE_ENV=test karma start test/karma.conf.js",
    "testwatch": "npm test -- --auto-watch --no-single-run",
    "preversion": "git update-index --no-assume-unchanged package-lock.json",
    "postversion": "git update-index --assume-unchanged package-lock.json && git push --tags && git push",
    "config": "cross-env NODE_ENV=dev NODE_ENV_MODE=config tasks/setupConfig.js",
    "start": "./node_modules/proton-bundler/scripts/createVersionJSON.sh && cross-env NODE_ENV=dev tasks/setupConfig.js",
    "poststart": "npm run dev",
    "start:raw": "./node_modules/proton-bundler/scripts/createVersionJSON.sh && cross-env NODE_ENV=dev tasks/setupConfig.js",
    "poststart:raw": "npm run dev -- --no-transpile",
    "dev": "cross-env NODE_ENV=dev && webpack-dev-server --hot --progress",
    "build": "cross-env NODE_ENV=dist webpack",
    "deploy": "cross-env NODE_ENV=dist proton-bundler --default-branch v3",
    "deployProd": "npm run deploy -- --branch=deploy-prod-a --flow=many",
    "build:ci": "cross-env NODE_ENV=dist NODE_ENV_DIST=ci tasks/deploy.js",
    "releaser:unshift": "./tasks/unshiftReleaser.sh",
    "i18n:check": "cross-env APP_KEY=Angular  proton-i18n validate",
    "i18n:compile": "cross-env APP_KEY=Angular  proton-i18n compile",
    "i18n:sync": "cross-env APP_KEY=Angular proton-i18n crowdin -s && npm run i18n:compile",
    "i18n:export": "cross-env APP_KEY=Angular proton-i18n crowdin -e",
    "i18n:update": "cross-env APP_KEY=Angular proton-i18n crowdin -u",
    "i18n:extract": "cross-env APP_KEY=Angular proton-i18n extract",
    "i18n:upgrade": "npm run i18n:extract && npm run i18n:update",
    "posti18n:upgrade": "cross-env APP_KEY=Angular proton-i18n commit update",
    "posti18n:sync": "cross-env APP_KEY=Angular proton-i18n list && cross-env APP_KEY=Angular proton-i18n commit upgrade",
    "pretty": "prettier --write $(find src/app -type f -name '*.js')",
    "create": "appComponent",
    "create:test": "appComponent --test",
    "cypress:open": "./tasks/lazyInstallVendor e2e && cypress open",
    "i18n:getlatest": "cross-env APP_KEY=Angular proton-i18n upgrade --custom",
    "bundle": "./node_modules/proton-bundler/scripts/createVersionJSON.sh && cross-env NODE_ENV=dev tasks/setupConfig.js",
    "postbundle": "cross-env NODE_ENV_DIST=bundle-only NODE_ENV=dist proton-bundler"
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "prettier --write",
        "git add"
      ],
      "env/env.json": [
        "tasks/checkEnv.js"
      ]
    },
    "ignore": [
      "static/*"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "version": "3.16.19",
  "husky": {
    "hooks": {
      "pre-commit": "git update-index --assume-unchanged package-lock.json && ./tasks/metrics.sh"
    }
  }
